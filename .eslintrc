{
    "env": {
        "browser": true,
        "node": true,
        "amd": true
    },
    "globals": {
        "DEBUG": false
    },
    "rules": {
        "no-alert": 2,
        "no-array-constructor": 2,
        "no-caller": 2,
        "no-catch-shadow": 2,
        "no-cond-assign": 2,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-delete-var": 2,
        "no-dupe-keys": 2,
        "no-else-return": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-empty-label": 2,
        "no-eval": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-semi": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-func-assign": 2,
        "no-implied-eval": 2,
        "no-inner-declarations": [2, "functions"],
        "no-invalid-regexp": 2,
        "no-iterator": 2,
        "no-label-var": 2,
        "no-lone-blocks": 2,
        "no-lonely-if": 2,
        "no-mixed-requires": [2, false],
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-negated-in-lhs": 2,
        "no-nested-ternary": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-object": 2,
        "no-new-wrappers": 2,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-proto": 2,
        "no-regex-spaces": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-shadow": 2,
        "no-shadow-restricted-names": 2,
        "no-spaced-func": 2,
        "no-sparse-arrays": 2,
        "no-undef": 2,
        "no-undef-init": 2,
        "no-unreachable": 2,
        "no-unused-expressions": 2,
        "no-unused-vars": [
            2,
            {
                "vars": "all",
                "args": "after-used"
            }
        ],
        "no-warning-comments": [
            0,
            {
                "terms": [
                    "todo",
                    "fixme",
                    "xxx"
                ],
                "location": "start"
            }
        ],
        "no-with": 2,
        "block-scoped-var": 2,
        "brace-style": [0, "1tbs"],
        "complexity": [0, 11],
        "consistent-return": 2,
        "curly": [0, "multi"],
        "default-case": 2,
        "dot-notation": 2,
        "eqeqeq": 2,
        "func-style": [0, "declaration"],
        "max-depth": [0, 4],
        "max-len": [0, 80, 4],
        "max-nested-callbacks": [2, 3],
        "max-params": [0, 3],
        "max-statements": [ 0, 10],
        "handle-callback-err": 0,
        "new-cap": 2,
        "new-parens": 2,
        "quotes": [2, "double"],
        "semi": 2,
        "semi-spacing": [2, { "before": false, "after": true }],
        "space-after-keywords": [0, "always"],
        "space-in-brackets": [0, "never"],
        "space-infix-ops": 2,
        "space-return-throw-case": 2,
        "strict": [2, "global"],
        "use-isnan": 2,
        "valid-typeof": 2,
        "wrap-iife": [2, "outside"]
    }
}
